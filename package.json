{
  "name": "extract-typings",
  "version": "1.0.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "extract-typings": "dist/bin.cjs"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build && npm run typings",
    "lint": "eslint --ext .ts,.tsx ./src",
    "typings": "node dist/bin.cjs -e src/index.ts -o dist/typings -c"
  },
  "keywords": [
    "typescript",
    "typings",
    "extract"
  ],
  "license": "MIT",
  "description": "Extract typings from TypeScript files",
  "author": "lei.li",
  "homepage": "https://github.com/wesmile2020/extract-typings",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesmile2020/extract-typings.git"
  },
  "bugs": {
    "url": "https://github.com/wesmile2020/extract-typings/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.0",
    "eslint": "^9.37.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "npm:rolldown-vite@7.2.9",
    "vite-plugin-checker": "^0.11.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.9"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "convert_json2dts": "^1.0.4",
    "ora": "^9.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
